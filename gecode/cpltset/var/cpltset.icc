  forceinline CpltSet::CpltSetVarImp*
  CpltSetVar::variable(void) const { return var; }

  forceinline bool
  CpltSetVar::contains(int i) const { return var->knownIn(i); }

  forceinline bool
  CpltSetVar::notContains(int i) const { return var->knownOut(i); }

  forceinline bool
  CpltSetVar::assigned(void) const { return var->assigned(); }

  forceinline int
  CpltSetVar::valididx(int v) const { return var->valididx(v); }

  forceinline unsigned int
  CpltSetVar::offset(void) const { return var->offset(); }

  forceinline unsigned int
  CpltSetVar::table_width(void) const { return var->table_width(); }

  forceinline bdd
  CpltSetVar::getbdd(int i) const { return var->getbdd(i); }

  forceinline bdd
  CpltSetVar::getnegbdd(int i) const { return var->getnegbdd(i); }

  forceinline int
  CpltSetVar::getlevel(int i)  { return var->getlevel(i); }

  forceinline int
  CpltSetVar::getvar(int i)  { return var->getvar(i); }

  forceinline int
  CpltSetVar::mgr_min(void) const { return var->mgr_min(); }

  forceinline int
  CpltSetVar::mgr_max(void) const { return var->mgr_max(); }

  forceinline void
  CpltSetVar::update(Space* home, bool share, CpltSetVar& x) { 
    var = x.var->copy(home,share); 
  }

  forceinline unsigned int
  CpltSetVar::cardMin(void) const { return var->cardMin(); };

  forceinline unsigned int 
  CpltSetVar::cardMax(void) const { return var->cardMax(); };

