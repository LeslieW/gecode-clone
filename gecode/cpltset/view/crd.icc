  forceinline int
  CrdCpltSetView::glbMin(void) const { 
    BMI* mgr = manager();
    if (mgr->ctrue(bounds)) { return CpltSetVarImp::MIN_OF_EMPTY; }
    BddIterator iter(mgr, bounds);
    while (iter()) {
      if (iter.status() == FIX_GLB) {
	int idx = iter.label() - offset();
	// std::cout << "idx = " << idx << "\n";
	return mgr_min() + idx;
      }
      ++iter;
    }
    return CpltSetVarImp::MIN_OF_EMPTY;
  }

  forceinline int 
  CrdCpltSetView::glbMax(void) const {
    BMI* mgr = manager();
    if (mgr->ctrue(bounds)) { return CpltSetVarImp::MAX_OF_EMPTY; } 
    BddIterator iter(mgr, bounds);
    int lastglb = -1;
    while (iter()) {
      if (iter.status() == FIX_GLB) {
	int idx = iter.label() - offset();
	// std::cout << "idx = " << idx << "\n";
	lastglb = mgr_min() + idx;
      } 
      ++iter;
    }
    return (lastglb == -1) ? CpltSetVarImp::MAX_OF_EMPTY : lastglb;
  };

