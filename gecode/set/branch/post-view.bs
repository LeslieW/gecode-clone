[Header]

[Footer]

// STATISTICS: set-branch

[General]
# Type of view
View:		SetView
# Type of variable argument
VarArgs:	SetVarArgs
# Type of variable branching selection
VarBranch:	SetVarBranch
# Type of value branch selection
ValBranch:	SetValBranch
# Exception to be thrown if variable branch selection unknown
Exception:	UnknownBranching("Set::branch")
# Header file to include
Include:	<gecode/set/branch.hh>
# Local namespace
LNS:		Gecode::Set::Branch
# Global namespace
GNS:		Gecode
[VarBranch]
# Value for branching selection, mark as special
Value:		SET_VAR_NONE = NONE
# Corresponding type
Type:		ViewSelNone<SetView>
[VarBranch]
Value:		SET_VAR_RND = COMPLETE
Type:		ViewSelRnd<SetView>
[VarBranch]
Value:		SET_VAR_MIN_MIN
Type:		ViewSelMin<MeritMin>
[VarBranch]
Value:		SET_VAR_MIN_MAX
Type:		ViewSelMax<MeritMin>
[VarBranch]
Value:		SET_VAR_MAX_MIN
Type:		ViewSelMin<MeritMax>
[VarBranch]
Value:		SET_VAR_MAX_MAX
Type:		ViewSelMax<MeritMax>
[VarBranch]
Value:		SET_VAR_SIZE_MIN
Type:		ViewSelMin<MeritSize>
[VarBranch]
Value:		SET_VAR_SIZE_MAX
Type:		ViewSelMax<MeritSize>
[VarBranch]
Value:		SET_VAR_DEGREE_MIN
Type:		ViewSelMin<MeritDegree<SetView> >
[VarBranch]
Value:		SET_VAR_DEGREE_MAX
Type:		ViewSelMax<MeritDegree<SetView> >
[VarBranch]
Value:		SET_VAR_AFC_MIN
Type:		ViewSelMin<MeritAfc<SetView> >
[VarBranch]
Value:		SET_VAR_AFC_MAX
Type:		ViewSelMax<MeritAfc<SetView> >
[VarBranch]
Value:		SET_VAR_ACTIVITY_MIN
Type:		ViewSelMin<MeritActivity<SetView> >
[VarBranch]
Value:		SET_VAR_ACTIVITY_MAX
Type:		ViewSelMax<MeritActivity<SetView> >
[VarBranch]
Value:		SET_VAR_SIZE_DEGREE_MIN
Type:		ViewSelMin<MeritSizeDegree>
[VarBranch]
Value:		SET_VAR_SIZE_DEGREE_MAX
Type:		ViewSelMax<MeritSizeDegree>
[VarBranch]
Value:		SET_VAR_SIZE_AFC_MIN
Type:		ViewSelMin<MeritSizeAfc>
[VarBranch]
Value:		SET_VAR_SIZE_AFC_MAX
Type:		ViewSelMax<MeritSizeAfc>
[VarBranch]
Value:		SET_VAR_SIZE_ACTIVITY_MIN
Type:		ViewSelMin<MeritSizeActivity>
[VarBranch]
Value:		SET_VAR_SIZE_ACTIVITY_MAX
Type:		ViewSelMax<MeritSizeActivity>
[End]
