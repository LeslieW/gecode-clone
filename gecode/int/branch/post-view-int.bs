[Header]

[Footer]

// STATISTICS: int-branch

[General]
# Type of view
View:		IntView
# Type of variable argument
VarArgs:	IntVarArgs
# Type of variable branching selection
VarBranch:	IntVarBranch
# Type of value branch selection
ValBranch:	IntValBranch
# Type of branch filter function
BranchFilter:	IntBranchFilter
# Exception to be thrown if variable branch selection unknown
Exception:	UnknownBranching("Int::branch")
# Header file to include
Include:	<gecode/int/branch.hh>
# Local namespace
LNS:		Gecode::Int::Branch
# Global namespace
GNS:		Gecode
[VarBranch]
# Value for branching selection, mark as special
Value:		SEL_NONE = NONE
# Corresponding type
Type:		ViewSelNone<IntView>
[VarBranch]
Value:		SEL_RND = COMPLETE
Type:		ViewSelRnd<IntView>
[VarBranch]
Value:		SEL_MERIT_MIN
Type:		ViewSelMin<MeritFunction<IntView> >
[VarBranch]
Value:		SEL_MERIT_MAX
Type:		ViewSelMax<MeritFunction<IntView> >
[VarBranch]
Value:		SEL_MIN_MIN
Type:		ViewSelMin<MeritMin>
[VarBranch]
Value:		SEL_MIN_MAX
Type:		ViewSelMax<MeritMin>
[VarBranch]
Value:		SEL_MAX_MIN
Type:		ViewSelMin<MeritMax>
[VarBranch]
Value:		SEL_MAX_MAX
Type:		ViewSelMax<MeritMax>
[VarBranch]
Value:		SEL_SIZE_MIN
Type:		ViewSelMin<MeritSize>
[VarBranch]
Value:		SEL_SIZE_MAX
Type:		ViewSelMax<MeritSize>
[VarBranch]
Value:		SEL_DEGREE_MIN
Type:		ViewSelMin<MeritDegree<IntView> >
[VarBranch]
Value:		SEL_DEGREE_MAX
Type:		ViewSelMax<MeritDegree<IntView> >
[VarBranch]
Value:		SEL_AFC_MIN
Type:		ViewSelMin<MeritAfc<IntView> >
[VarBranch]
Value:		SEL_AFC_MAX
Type:		ViewSelMax<MeritAfc<IntView> >
[VarBranch]
Value:		SEL_ACTIVITY_MIN
Type:		ViewSelMin<MeritActivity<IntView> >
[VarBranch]
Value:		SEL_ACTIVITY_MAX
Type:		ViewSelMax<MeritActivity<IntView> >
[VarBranch]
Value:		SEL_SIZE_DEGREE_MIN
Type:		ViewSelMin<MeritSizeDegree>
[VarBranch]
Value:		SEL_SIZE_DEGREE_MAX
Type:		ViewSelMax<MeritSizeDegree>
[VarBranch]
Value:		SEL_SIZE_AFC_MIN
Type:		ViewSelMin<MeritSizeAfc>
[VarBranch]
Value:		SEL_SIZE_AFC_MAX
Type:		ViewSelMax<MeritSizeAfc>
[VarBranch]
Value:		SEL_SIZE_ACTIVITY_MIN
Type:		ViewSelMin<MeritSizeActivity>
[VarBranch]
Value:		SEL_SIZE_ACTIVITY_MAX
Type:		ViewSelMax<MeritSizeActivity>
[VarBranch]
Value:		SEL_REGRET_MIN_MIN
Type:		ViewSelMin<MeritRegretMin>
[VarBranch]
Value:		SEL_REGRET_MIN_MAX
Type:		ViewSelMax<MeritRegretMin>
[VarBranch]
Value:		SEL_REGRET_MAX_MIN
Type:		ViewSelMin<MeritRegretMax>
[VarBranch]
Value:		SEL_REGRET_MAX_MAX
Type:		ViewSelMax<MeritRegretMax>
[End]
